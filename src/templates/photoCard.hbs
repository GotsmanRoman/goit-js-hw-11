{{#each this.hits}}
  <div class='photo-card'>
    <img
      class='photo-card__image'
      src='{{webformatURL}}'
      alt=''
      loading='lazy'
    />
    <div class='info'>
      <p class='info-item'>Likes: {{likes}}</p>
      <p class='info-item'>Views: {{views}}</p>
      <p class='info-item'>Comments: {{comments}}</p>
      <p class='info-item'>Downloads: {{downloads}}</p>
    </div>
  </div>
{{/each}}